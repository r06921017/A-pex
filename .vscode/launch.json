{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) BOPS",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/multiobj",
            "args": [
                "--map","${workspaceFolder}/resources/dataset/USA-road-d.BAY.gr",
                "${workspaceFolder}/resources/dataset/USA-road-t.BAY.gr",
                "--start", "20002",  // "1000",
                "--goal", "164983",  // "1649", 
                "--algorithm","BOPS",
                "--cutoffTime", "300",
                "-f", "4",
                "--output","output.csv"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}